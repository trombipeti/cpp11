<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="SDLFuncDrawer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/SDLFuncDrawer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/SDLFuncDrawer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="`sdl-config --cflags`-mwindows" />
			<Add directory="$(#sdl.include)" />
		</Compiler>
		<Linker>
			<Add option="`sdl-config --libs` -lSDLmain -lSDL -lSDL_gfx -lmingw32" />
			<Add library="mingw32" />
			<Add library="SDLmain" />
			<Add library="SDL.dll" />
			<Add library="user32" />
			<Add library="winmm" />
			<Add library="gdi32" />
			<Add library="dxguid" />
			<Add library="opengl32" />
			<Add library="glu32" />
			<Add directory="C:/Program Files (x86)/CodeBlocks/MinGW/lib" />
			<Add directory="$(#sdl.lib)" />
		</Linker>
		<Unit filename="RPNParser.cpp" />
		<Unit filename="RPNParser.h" />
		<Unit filename="cb.bmp" />
		<Unit filename="main.cpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
